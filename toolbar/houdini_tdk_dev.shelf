  <tool name="tdk_dev::network_statistics" label="Show Network Statistics" icon="BUTTONS_info">
    <script scriptType="python"><![CDATA[import houdini_tdk as tdk

nodes = hou.selectedNodes()
if nodes:
    tdk.showStatsForNode(nodes[0])
]]></script>
